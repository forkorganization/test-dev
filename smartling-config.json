{
    "locales" : [
        {
            "smartling"   : "zh-CN",
            "application" : "zh_Hans"
        },
        {
            "smartling"   : "nl-NL",
            "application" : "nl_NL"
        },
        // Added new locale after files were submitted to smartling
        {
            "smartling"   : "ru-RU",
            "application" : "russ"
        }
    ],
    "resourceSets": [
        {
            "pathRegex"                 : "messages/(?<path>.*)\\.properties",
            "translationPathExpression" : "/translated/${path}_${locale}.${originalFile.extension}",
            "type"                      : "javaProperties"
        }
    ]
}
