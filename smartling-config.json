{
  "locales": [
    {
      "smartling": "zh-CN",
      "application": "zh_cn"
    },
    {
      "smartling": "nl-NL",
      "application": "nl"
    }
  ],
  "resourceSets": [
    {
      "pathRegex": "src\\/.*_en\\.json",
      "translationPathExpression": "${originalFile.path}${originalFile.baseName.replaceAll('en$', locale)}.${originalFile.extension}",
      "translationCommitMessage": "Translate ${originalFile.name} to ${locale}\n${translatedFile.fullName}",
      "type": "json",
      "authorizeContent": true
    }
  ]
}
